{
  "name": "phpunitgen/webapp",
  "description": "The PhpUnitGen Web Application.",
  "license": "MIT",
  "keywords": [
    "test",
    "generation",
    "generator"
  ],
  "support": {
    "issues": "https://github.com/paul-thebaud/phpunitgen-core/issues",
    "source": "https://github.com/paul-thebaud/phpunitgen-core"
  },
  "authors": [
    {
      "name": "Paul Thébaud",
      "email": "paul.thebaud29@gmail.com"
    },
    {
      "name": "Killian Hascoët",
      "email": "killianh@live.fr"
    }
  ],
  "require": {
    "php": "7.3.*",
    "ext-json": "*",
    "laravel/lumen-framework": "^6.0",
    "ocramius/package-versions": "^1.5",
    "phpunitgen/core": "^1.0-alpha"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/paul-thebaud/phpunitgen-core"
    }
  ],
  "autoload": {
    "files": [
      "app/helpers/functions.php"
    ],
    "psr-4": {
      "PhpUnitGen\\WebApp\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\PhpUnitGen\\WebApp\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
