{
  "name": "phpunitgen-webapp",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/paul-thebaud/phpunitgen-webapp.git",
  "author": "Paul Th√©baud <paul.thebaud29@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev-js": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack.js.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-js": "NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --env.mixfile=webpack.js.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot-js": "NODE_ENV=development webpack-dev-server --inline --hot --env.mixfile=webpack.js.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod-js": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack.js.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod-js-no-progress": "NODE_ENV=production node_modules/webpack/bin/webpack.js --hide-modules --env.mixfile=webpack.js.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "analyze-js": "NODE_ENV=production ANALYZE=true node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack.js.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev-css": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack.css.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-css": "NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --env.mixfile=webpack.css.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot-css": "NODE_ENV=development webpack-dev-server --inline --hot --env.mixfile=webpack.css.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod-css": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack.css.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod-css-no-progress": "NODE_ENV=production node_modules/webpack/bin/webpack.js --hide-modules --env.mixfile=webpack.css.mix.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "$npm_execpath run dev-css & $npm_execpath run dev-js",
    "watch": "concurrently --kill-others \"$npm_execpath run watch-css\" \"$npm_execpath run watch-js\"",
    "prod": "$npm_execpath run prod-css & $npm_execpath run prod-js",
    "prod-no-progress": "$npm_execpath run prod-css-no-progress & $npm_execpath run prod-js-no-progress",
    "build": "$npm_execpath run prod-no-progress",
    "lint": "node_modules/eslint/bin/eslint.js --ext .js,.vue,.ts resources/ts"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.20",
    "@fortawesome/free-regular-svg-icons": "^5.10.0",
    "@fortawesome/free-solid-svg-icons": "^5.10.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/vue-clickaway": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "@vue/eslint-config-typescript": "^5.0.0",
    "autoprefixer": "^9.7.2",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.28",
    "codemirror": "^5.49.2",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "docsify": "^4.9.4",
    "docsify-copy-code": "^2.1.0",
    "docsify-pagination": "^2.5.0",
    "eslint": "^6.6.0",
    "eslint-plugin-vue": "^6.0.1",
    "inversify": "^5.0.1",
    "laravel-mix": "^5.0.0",
    "laravel-mix-merge-manifest": "^0.1.2",
    "lodash.debounce": "^4.0.8",
    "marked": "^0.7.0",
    "reflect-metadata": "^0.1.13",
    "resolve-url-loader": "3.1.0",
    "sass": "^1.22.7",
    "sass-loader": "^8.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.0.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-clickaway": "^2.2.2",
    "vue-clipboard2": "^0.3.1",
    "vue-codemirror": "^4.0.6",
    "vue-gtag": "^1.1.0",
    "vue-i18n": "^8.14.0",
    "vue-property-decorator": "^8.3.0",
    "vue-radial-progress": "^0.2.10",
    "vue-router": "^3.0.7",
    "vue-scrollto": "^2.17.1",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-chunk-rename-plugin": "^1.0.3",
    "whatwg-fetch": "^3.0.0"
  }
}
